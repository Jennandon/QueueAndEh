<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/accent_material_dark">

    <Button
        android:text="NOPE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/NO"
        android:layout_alignBaseline="@+id/YES"
        android:layout_alignBottom="@+id/YES"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="40dp"
        android:layout_marginEnd="40dp"
        android:fontFamily="monospace" />

    <Button
        android:text="YEAH"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/YES"
        android:layout_marginLeft="38dp"
        android:layout_marginStart="38dp"
        android:layout_marginBottom="19dp"
        android:layout_above="@+id/button5"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:fontFamily="monospace"
        style="@android:style/Widget.DeviceDefault.Light.Button" />

    <Button
        android:text="start again"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button5"
        style="@android:style/Widget.Material.Button.Borderless.Colored"
        android:layout_marginRight="15dp"
        android:layout_marginEnd="15dp"
        android:fontFamily="monospace"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/NO"
        android:layout_alignEnd="@+id/NO" />

    <Button
        android:text="go back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/back"
        style="@android:style/Widget.Material.Button.Borderless.Colored"
        android:fontFamily="monospace"
        android:layout_toLeftOf="@+id/button5"
        android:layout_toStartOf="@+id/button5"
        android:layout_marginRight="36dp"
        android:layout_marginEnd="36dp"
        android:visibility="visible"
        android:layout_alignParentBottom="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/city1"
        android:id="@+id/imageView10"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/indecicitytitle"
        android:id="@+id/imageView9"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="55dp" />

    <TextView
        android:text="ARE YOU FEELING"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView3"
        android:textSize="24sp"
        android:textColor="@color/abc_input_method_navigation_guard"
        android:fontFamily="monospace"
        android:layout_marginTop="35dp"
        android:textAlignment="center"
        android:layout_below="@+id/imageView9"
        android:layout_centerHorizontal="true" />

    <TextView
        android:text="INDECISIVE?"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView5"
        android:textSize="23sp"
        android:fontFamily="monospace"
        android:textColor="@color/abc_input_method_navigation_guard"
        android:layout_below="@+id/textView3"
